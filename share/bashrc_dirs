# Source this in your .bashrc

# Nicknames for common dirs
export PGK="/pool/genomics/$USER"
export SAB="/share/apps/bioinformatics"
export SAMB="/share/apps/modulefiles/bioinformatics"
export DG="/data/genomics"
export DGK="/data/genomics/$USER"
export PG="/pool/genomics"
export SGK="/scratch/genomics/$USER"
export SG="/scratch/genomics"
export SL="/scratch/nmnh_lab"
export SLK="/scratch/nmnh_lab/$USER"
export SSFK="/scratch/schultzt/fungal_UCEs/$USER/"
export SSFKTT="$SSFK/trying_taxasets"
export SSFKTP="$SSFK/trying_programs"
export SSFKTA="$SSFK/trying_analyses"
export SSFKFA="$SSFK/final_analyses"

# Aliases to go to those common dirs
alias pgk="cd $PGK"
alias sab="cd $SAB"
alias samb="cd $SAMB"
alias dg="cd $DG"
alias dgk="cd $DGK"
alias pg="cd $PG"
alias sgk="cd $SGK"
alias sg="cd $SG"
alias ssfk="cd $SSFK"
alias ssfktt="cd $SSFKTT"
alias ssfktp="cd $SSFKTP"
alias ssfkta="cd $SSFKTA"
alias ssfkfa="cd $SSFKFA"
alias sl="cd $SL"
alias slk="cd $SLK"
